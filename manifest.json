{
	"manifest_version": 3,
	"name": "Browser Sidecar",
	"description": "",
	"short_name":"SideCar",
	"author": "Tai Groot",
	"version": "0.0.1",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"activeTab",
		"browser_action",
		"scripting",
		"tabs",
		"webNavigation"
	],
	"commands":{
		"_execute_browser_action":{
			"suggested_key":{
				"default": "Ctrl+Shift+Z"
			}
		}
	},
	"action":{},
	"icons": {
		"16": "static/icon-16.png",
		"32": "static/icon-32.png",
		"48": "static/icon-48.png",
		"64": "static/icon-64.png",
		"128": "static/icon-128.png"
	}
}
